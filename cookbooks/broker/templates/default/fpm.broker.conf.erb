[ajk-broker]

listen = 127.0.0.1:9000
user = <%= @phpuser %>
group = <%= @phpgroup %>
pm = dynamic
pm.max_children = 5
pm.start_servers = 2
pm.min_spare_servers = 1
pm.max_spare_servers = 5
php_value[extension] = bcmath
php_value[extension] = apc
php_value[extension] = memcache
php_value[extension] = msgpack
php_value[extension] = gd
php_value[extension] = mbstring
php_value[extension] = php-zmq

