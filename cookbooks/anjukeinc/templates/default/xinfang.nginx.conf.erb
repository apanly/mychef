server {
    listen 80;
    server_name .local.dev.xinfang.anjuke.com .<%= @subdomain %>.dev.xinfang.anjuke.com;
    proxy_buffering off;
    proxy_set_header Host $host;
    proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;

    location /{
        rewrite . /index.php last;
    }

    location /index.php {
        root /home/<%= @docuser %>/www/indexes/xinfang.php;
        internal;
        fastcgi_pass 127.0.0.1:9003;
        fastcgi_param SCRIPT_FILENAME /home/<%= @docuser %>/www/indexes/xinfang.php;
        include fastcgi_params;
        expires epoch;
    }
}

