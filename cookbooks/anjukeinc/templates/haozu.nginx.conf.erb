server {
    listen 80;
    server_name .local.dev.haozu.com .<%= @subdomain %>.dev.haozu.com;
    proxy_buffering off;
    proxy_set_header Host $host;
    proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;

    location /{
        rewrite . /index.php last;
    }

    location /index.php {
        root /home/vagrant/www/indexes/haozu.php;
        internal;
        fastcgi_pass 127.0.0.1:9000;
        fastcgi_param SCRIPT_FILENAME /home/vagrant/www/indexes/haozu.php;
        include fastcgi_params;
        expires epoch;
    }
}

